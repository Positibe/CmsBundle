{% extends '@PositibeCms/layout.html.twig' %}

{% block title %}Bloques de menu{% endblock %}
{% block subtitle %}Crear nuevo bloque de menu{% endblock %}
{% set menu_active  = 'block'%}
{% set content_menu = 'menu' %}

{% block breadcrumb_list %}
    <li class="breadcrumb-item">

        <a href="{{ path('positibe_menu_block_index') }}">Bloques de menu</a>
    </li>
    <li class="breadcrumb-item">

        Nuevo bloque de menu
    </li>
{% endblock %}

{% block content %}
    {{ form_start(form,{'attr': {'class': 'inbox-compose form-horizontal' }}) }}
    {{ include('@PositibeCms/MenuBlock/_form.html.twig') }}
    {{ form_end(form) }}
{% endblock %}

{% block javascripts %}
    {{ include('@PositibeCms/Block/_form_always_select.html.twig') }}
{% endblock %}